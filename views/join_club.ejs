<%- include('./partials/head') %>

    <h1><%= title %></h1>
    <p>Enter the secret code for private access!</p>
    <form method='post'>
      <div class='form-input'>
        <div>
          <label for='private_access'>Access Code: </label>
          <input type='password' name='private_access' id='private_access'>
          <button type='submit'>Join</button>
        </div>
      </div>

      <% if (typeof errors !== 'undefined') { %>
       <% errors.forEach(error => { %>
        <p><%= error.msg %></p>
       <% }) %>
      <% } %>

    </form>

    <%= typeof currentUser === 'undefined' ? '' : currentUser %>
<%- include('./partials/footer') %>